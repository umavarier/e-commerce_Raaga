    <section class="login spad">
        <!-- <div class="container"> -->
            <div class="row">
                <div class="col-lg-6">
                    <div class="login__form">
                        <h3>Login</h3>


                        <form action="/login" method="post" onsubmit="return allChecking()">


                            <div class="">
                                <% if(typeof message !=='undefined' ){ %>
                                  <span class="fs-4">
                                    <span style="color: red;">
                                      <%=message %>
                                    </span>
                                  </span>
                                  <% } %>
                              </div>
                              <br>

                            <div class="input__item">
                                <input type="text" placeholder="Email address" id="email" name="email" onchange="emailValidate()">
                                <span class="icon_mail"></span>
                            </div>
                            <div class="input__item">
                                <input type="password" placeholder="Password" id="password" name="password" onchange="passwordValidate()" >
                                <span class="icon_lock"></span>
                            </div>
                            <button type="submit" class="site-btn" onclick="emailValidate(),passwordValidate()">Login Now</button>

                        </form>
                        <a class="text-decoration-none" href="/forgot-P">forgot password?</a>
                    </div>
